pipeline {
    agent any

    stages {
        stage('Checkout Code') {
            steps {
        git url: 'https://github.com/Anand-poola/ProjectforCD.git', branch: 'main'
    }
        }

        stage('Build with Maven') {
            steps {
        echo "Running Maven build..."
        sh 'mvn clean install -Dmaven.test.failure.ignore=true'
    }
}
    
         
    }
}

