    agent any

    stages {
        stage('Checkout Code') {
    steps {
        withCredentials([string(credentialsId: 'github-cred', variable: 'T')]) {
            git url: "https://${T}:x-oauth-basic@github.com/Anand-poola/ProjectforCD.git", branch: 'main'
        }
    }
}
         
    }
}

